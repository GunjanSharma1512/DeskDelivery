<?xml version="1.0" encoding="utf-8"?>
<androidx.Constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:showIn="@layout/app_bar_main"
        tools:context=".MainActivity">
    <TextView
            android:text="South Indian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="250dp"
            android:textStyle="bold"
            android:textSize="20sp"
            android:background="@null"
            tools:layout_editor_absoluteX="63dp" android:id="@+id/textView2"/>
    <TextView
            android:text="North Indian"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="250dp"
            android:textStyle="bold"
            android:textSize="20sp"
            android:background="@null"
            tools:layout_editor_absoluteX="231dp" android:id="@+id/textView3"/>
    <TextView
            android:text="Chinese"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="474dp"
            android:textStyle="bold"
            android:textSize="20sp"
            android:background="@null"
            tools:layout_editor_absoluteX="79dp" android:id="@+id/textView4"/>
    <TextView
            android:text="Juice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:layout_editor_absoluteY="474dp"
            android:textStyle="bold"
            android:textSize="20sp"
            android:background="@null"
            tools:layout_editor_absoluteX="258dp" android:id="@+id/textView5"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@mipmap/ic_launcher_foreground_southindian"
            tools:layout_editor_absoluteY="109dp" tools:layout_editor_absoluteX="53dp" android:id="@+id/imageButton7"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@mipmap/ic_launcher_foreground_northindian"
            tools:layout_editor_absoluteY="109dp" tools:layout_editor_absoluteX="223dp" android:id="@+id/imageButton8"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@mipmap/ic_launcher_foreground_chinesefood"
            tools:layout_editor_absoluteY="328dp" tools:layout_editor_absoluteX="53dp" android:id="@+id/imageButton9"/>
    <ImageButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@mipmap/ic_launcher_foreground_juice"
            tools:layout_editor_absoluteY="328dp" tools:layout_editor_absoluteX="223dp"
            android:id="@+id/imageButton10"/>

</androidx.Constraintlayout.widget.ConstraintLayout>